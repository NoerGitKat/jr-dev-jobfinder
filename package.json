  "description": "An app that pulls data from Indeed/GitHub/StackOverflow to easily access all Junior Dev jobs in one place!",
  "main": "index.js",
  "scripts": {
    "start": "node ./server/server.js",
    "server": "nodemon ./server/server.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\" "
