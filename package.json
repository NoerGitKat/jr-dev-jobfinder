  "bugs": {
    "url": "https://github.com/NoerGitKat/jr-dev-jobfinder/issues"
  },
  "homepage": "https://github.com/NoerGitKat/jr-dev-jobfinder#readme",
  "dependencies": {
    "concurrently": "^5.0.2",
    "cron": "^1.7.2",
    "express": "^4.17.1",
    "node-fetch": "^2.6.0",
    "redis": "^2.8.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
}
